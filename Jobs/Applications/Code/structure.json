    "query": {
        "from": {
            "table": "Jobs\\Jobs\\Applications\\Code\\Tables\\Applications",
            "alias": "jja",
            "name": "#__jobs_jobs_applications"
        },
        "select": [
            "jja.id",
            "jja.job_id",
            "jj.title AS job_id_title",
            "jja.subject",
            "jja.message",
            "jja.phone",
            "jja.email",
            "jja.created_by",
            "uu.name AS created_by_name",
            "jja.date_created",
            "jja.modified_by",
            "uu_9685.name AS modified_by_name",
            "jja.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Jobs\\Jobs\\Code\\Tables\\Jobs",
                "alias": "jj",
                "name": "#__jobs_jobs",
                "on": "jja.job_id = jj.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "jja.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_9685",
                "name": "#__users_users",
                "on": "jja.modified_by = uu_9685.id"
            }
        ]
    }
