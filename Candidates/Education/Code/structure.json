    "query": {
        "from": {
            "table": "Jobs\\Candidates\\Education\\Code\\Tables\\Education",
            "alias": "jce",
            "name": "#__jobs_candidates_education"
        },
        "select": [
            "jce.id",
            "jce.institution",
            "jce.candidate_id",
            "jc.name AS candidate_id_name",
            "jce.certification_id",
            "jc_9238.title AS certification_id_title",
            "jce.field_of_study",
            "jce.grade",
            "jce.description",
            "jce.activities",
            "jce.start_date",
            "jce.end_date",
            "jce.created_by",
            "uu.name AS created_by_name",
            "jce.date_created",
            "jce.modified_by",
            "uu_2275.name AS modified_by_name",
            "jce.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Jobs\\Candidates\\Code\\Tables\\Candidates",
                "alias": "jc",
                "name": "#__jobs_candidates",
                "on": "jce.candidate_id = jc.id"
            },
            {
                "table": "Jobs\\Certifications\\Code\\Tables\\Certifications",
                "alias": "jc_9238",
                "name": "#__jobs_certifications",
                "on": "jce.certification_id = jc_9238.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "jce.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_2275",
                "name": "#__users_users",
                "on": "jce.modified_by = uu_2275.id"
            }
        ]
    }
