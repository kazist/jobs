    "query": {
        "from": {
            "table": "Jobs\\Candidates\\Skills\\Code\\Tables\\Skills",
            "alias": "jcs",
            "name": "#__jobs_candidates_skills"
        },
        "select": [
            "jcs.id",
            "jcs.candidate_id",
            "jc.name AS candidate_id_name",
            "jcs.skill_id",
            "js.skill AS skill_id_skill",
            "jcs.created_by",
            "uu.name AS created_by_name",
            "jcs.date_created",
            "jcs.modified_by",
            "uu_939.name AS modified_by_name",
            "jcs.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Jobs\\Candidates\\Code\\Tables\\Candidates",
                "alias": "jc",
                "name": "#__jobs_candidates",
                "on": "jcs.candidate_id = jc.id"
            },
            {
                "table": "Jobs\\Skills\\Code\\Tables\\Skills",
                "alias": "js",
                "name": "#__jobs_skills",
                "on": "jcs.skill_id = js.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "jcs.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_939",
                "name": "#__users_users",
                "on": "jcs.modified_by = uu_939.id"
            }
        ]
    }
