    "query": {
        "from": {
            "table": "Jobs\\Candidates\\Categories\\Code\\Tables\\Categories",
            "alias": "jcc",
            "name": "#__jobs_candidates_categories"
        },
        "select": [
            "jcc.id",
            "jcc.candidate_id",
            "jc.name AS candidate_id_name",
            "jcc.category_id",
            "jc_4590.title AS category_id_title",
            "jcc.created_by",
            "uu.name AS created_by_name",
            "jcc.date_created",
            "jcc.modified_by",
            "uu_6230.name AS modified_by_name",
            "jcc.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Jobs\\Candidates\\Code\\Tables\\Candidates",
                "alias": "jc",
                "name": "#__jobs_candidates",
                "on": "jcc.candidate_id = jc.id"
            },
            {
                "table": "Jobs\\Categories\\Code\\Tables\\Categories",
                "alias": "jc_4590",
                "name": "#__jobs_categories",
                "on": "jcc.category_id = jc_4590.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "jcc.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_6230",
                "name": "#__users_users",
                "on": "jcc.modified_by = uu_6230.id"
            }
        ]
    }
