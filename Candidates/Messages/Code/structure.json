    "query": {
        "from": {
            "table": "Jobs\\Candidates\\Messages\\Code\\Tables\\Messages",
            "alias": "jcm",
            "name": "#__jobs_candidates_messages"
        },
        "select": [
            "jcm.id",
            "jcm.candidate_id",
            "jc.name AS candidate_id_name",
            "jcm.subject",
            "jcm.message",
            "jcm.email",
            "jcm.created_by",
            "uu.name AS created_by_name",
            "jcm.date_created",
            "jcm.modified_by",
            "uu_5813.name AS modified_by_name",
            "jcm.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Jobs\\Candidates\\Code\\Tables\\Candidates",
                "alias": "jc",
                "name": "#__jobs_candidates",
                "on": "jcm.candidate_id = jc.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "jcm.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5813",
                "name": "#__users_users",
                "on": "jcm.modified_by = uu_5813.id"
            }
        ]
    }
